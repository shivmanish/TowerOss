<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="input_style" >

        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#FFFFFF</item>

    </style>

    <style name="TextInputLayout" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense">
        <item name="boxStrokeColor">#ffffff</item>
        <item name="boxStrokeWidth">1dp</item>
        <item name="android:padding">3dp</item>
        <item name="boxCornerRadiusBottomEnd">10dp</item>
        <item name="boxCornerRadiusBottomStart">10dp</item>
        <item name="boxCornerRadiusTopEnd">10dp</item>
        <item name="boxCornerRadiusTopStart">10dp</item>
        <item name="backgroundTint">#ffffff</item>
        <item name="hintTextColor">#ffffff</item>
        <item name="colorErrorContainer">@color/transparent</item>
        <item name="errorIconDrawable">@drawable/icon_error</item>
        <item name="boxStrokeErrorColor">@color/white</item>
        <item name="errorTextColor">#ff5555</item>

    </style>

    <style name="ErrorText" parent="Widget.MaterialComponents.TextView">
        <item name="android:textSize">15sp</item>
        <item name="fontFamily">@font/poppins</item>
    </style>

    <style name="CustomTextInputEdit" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="android:colorPrimary">@color/white</item>
        <item name="android:textSize">15dp</item>

    </style>

    <style name="CustomEditField" parent="Widget.Design.TextInputLayout">
        <item name="colorAccent">@color/white</item>
    </style>
<!--comment added-->
    <style name="AutoResizeTextView" parent="Widget.AppCompat.TextView">
        <item name="autoSizeTextType">uniform</item>
        <item name="autoSizeMinTextSize">8sp</item>
        <item name="autoSizeMaxTextSize">100sp</item>
        <item name="android:textSize">100sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="autoSizePresetSizes">@array/autosize_text_sizes</item>
    </style>
    <style name="SpinnerTheme" parent="android:Widget.Spinner">
        <item name="android:background">@drawable/spinner_bg</item>
        <item name="windowMinWidthMajor">0dp</item>
        <item name="android:spinnerMode">dropdown</item>
        <item name="android:gravity">left</item>
        <item name="windowMinWidthMinor">0dp</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:textSize">@dimen/dp12</item>
        <item name="popupTheme">@style/SpinnerPopUpStyle</item>
        <item name="android:entries">@array/spinner_item</item>
        <item name="android:dropDownWidth">wrap_content</item>
        <item name="android:dropDownItemStyle">@style/DropDownTextView</item>

    </style>

    <style name="SpinnerPopUpStyle" parent="ThemeOverlay.AppCompat.Light">
        <!-- border width -->
        <item name="android:padding">1dp</item>
        <!-- border color -->
        <item name="android:background">@color/white</item>
    </style>

    <style name="DropDownTextView" parent="Widget.AppCompat.TextView">
        <item name="android:gravity">left</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:background">@color/card_bg</item>
        <item name="fontFamily">@font/poppins</item>
        <item name="android:textColor">#818181</item>
        <item name="android:textSize">@dimen/sp14</item>
        <item name="android:maxLines">1</item>
        <item name="android:singleLine">true</item>

    </style>
    <style name="TabTextView" parent="Widget.AppCompat.TextView">
        <item name="android:textSize">15sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:textColor">@color/text_color</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_width">match_parent</item>

    </style>
    <style name="TabHomeTextView" parent="Widget.AppCompat.TextView">
        <item name="android:textSize">15sp</item>
        <item name="android:textAllCaps">false</item>

    </style>

    <style name="RowTestItemEdit" parent="Widget.AppCompat.TextView">
        <item name="fontFamily">@font/poppins</item>
        <item name="android:textColor">@color/color1</item>
        <item name="fontWeight">400</item>
        <item name="android:textSize">@dimen/sp14</item>
        <item name="android:maxLines">1</item>
        <item name="android:background">@color/white</item>
        <item name="android:imeOptions">actionDone</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="RowTestItemEditSmall" parent="Widget.AppCompat.TextView">
        <item name="fontFamily">@font/poppins</item>
        <item name="android:hint">...</item>
        <item name="hintTextColor">@color/grey</item>
        <item name="android:textColor">@color/color1</item>
        <item name="fontWeight">400</item>
        <item name="android:textSize">@dimen/sp12</item>
        <item name="android:maxLines">1</item>
        <item name="android:background">@color/white</item>
        <item name="android:imeOptions">actionDone</item>
        <item name="android:singleLine">true</item>
    </style>


    <style name="MySwitch" parent="@style/Widget.AppCompat.CompoundButton.Switch">
        <item name="android:textColorPrimaryDisableOnly">#b71c1c</item>
        <item name="android:colorControlActivated">#10BE10</item>
        <item name="android:colorForeground">#323C47</item>
        <item name="android:textAppearance">@style/TextAppearance.AppCompat</item>
    </style>

    <style name="RowTextviewItem" parent="Widget.AppCompat.TextView">
