<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:id="@+id/collapsing_layout_smps"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:gravity="center_vertical"
        android:orientation="vertical"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:id="@+id/smps_root"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/expand_colaps_background"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/item_title_smps"
                style="@style/RowTestItemEdit"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@color/transparent"
                android:clickable="false"
                android:padding="@dimen/dp15"
                android:text="SMPS" />

            <ImageView
                android:id="@+id/smps_edit"
                android:layout_width="@dimen/dp50"
                android:layout_height="@dimen/dp50"
                android:layout_gravity="center_vertical"
                android:padding="@dimen/dp18"
                android:src="@drawable/edit_fav_btn"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/smps_arrow"
                android:layout_width="@dimen/dp50"
                android:layout_height="@dimen/dp50"
                android:layout_gravity="center_vertical"
                android:clickable="false"
                android:padding="@dimen/dp18"
                android:src="@drawable/ic_arrow_down_black" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/item_collapse_ac"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:orientation="vertical"
            android:visibility="gone">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="@dimen/dp10">

                <GridLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:columnCount="2"
                    android:orientation="horizontal"
                    app:layout_constraintTop_toTopOf="parent">

                    <LinearLayout
                        style="@style/ColumnItemInfo"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content">

                        <TextView
                            style="@style/RowTextItemInfo"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Make" />

                        <TextView
                            android:id="@+id/ac_make"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp20"
                            android:text="."
                            android:textColor="@color/black" />

                    </LinearLayout>

                    <LinearLayout
                        style="@style/ColumnItemInfo"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content">

                        <TextView
                            style="@style/RowTextItemInfo"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Modal" />

                        <TextView
                            android:id="@+id/ac_modal"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp20"
                            android:text="0"
                            android:textColor="@color/black" />

                    </LinearLayout>

                    <LinearLayout
                        style="@style/ColumnItemInfo"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content">

                        <TextView
                            style="@style/RowTextItemInfo"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Rating/ Capacity (KW)" />

                        <TextView
                            android:id="@+id/ac_Rating_capacity_kw"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp20"
                            android:text="22"
                            android:textColor="@color/black" />

                    </LinearLayout>

                    <LinearLayout
                        style="@style/ColumnItemInfo"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content">

                        <TextView
                            style="@style/RowTextItemInfo"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Cabinet Size" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/ac_cabinetsizel"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="L - "
                                    android:textColor="@color/black" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/ac_cabinetsizeb"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text=" | B - "
                                    android:textColor="@color/black" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/ac_cabinetsizeh"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text=" | H - "
                                    android:textColor="@color/black" />

                            </LinearLayout>

                        </LinearLayout>


                    </LinearLayout>

                    <LinearLayout
                        style="@style/ColumnItemInfo"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content">

                        <TextView
                            style="@style/RowTextItemInfo"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Indore Unit Size" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/ac_indore_Unit_sizel"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="L - "
                                    android:textColor="@color/black" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/ac_indore_Unit_sizeb"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text=" | B - "
                                    android:textColor="@color/black" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/ac_indore_Unit_sizeh"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text=" | H - "
                                    android:textColor="@color/black" />

                            </LinearLayout>

                        </LinearLayout>


                    </LinearLayout>

                    <LinearLayout
                        style="@style/ColumnItemInfo"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content">

                        <TextView
                            style="@style/RowTextItemInfo"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Indore Unit Weight" />

                        <TextView
                            android:id="@+id/ac_indore_unit_weight"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp20"
                            android:text="22"
                            android:textColor="@color/black" />

                    </LinearLayout>

                    <LinearLayout
                        style="@style/ColumnItemInfo"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content">

                        <TextView
                            style="@style/RowTextItemInfo"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Outdoor Unit Size" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/ac_outdoor_unit_sizel"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="L - "
                                    android:textColor="@color/black" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/ac_outdoor_unit_sizeb"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text=" | B - "
                                    android:textColor="@color/black" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/ac_outdoor_unit_sizeh"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text=" | H - "
                                    android:textColor="@color/black" />

                            </LinearLayout>

                        </LinearLayout>


                    </LinearLayout>

                    <LinearLayout
                        style="@style/ColumnItemInfo"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content">

                        <TextView
                            style="@style/RowTextItemInfo"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Outdoor Unit Weight" />

                        <TextView
                            android:id="@+id/ac_outdoor_unit_weight"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp20"
                            android:text="Cloudextel"
                            android:textColor="@color/black" />
                    </LinearLayout>

                    <LinearLayout
                        style="@style/ColumnItemInfo"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content">

                        <TextView
                            style="@style/RowTextItemInfo"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Overall Weight (KG)" />

                        <TextView
                            android:id="@+id/ac_overall_weight"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp20"
                            android:text="Airtel"
                            android:textColor="@color/black" />

                    </LinearLayout>

                    <LinearLayout
                        style="@style/ColumnItemInfo"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content">

                        <TextView
                            style="@style/RowTextItemInfo"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Owner Company" />

                        <TextView
                            android:id="@+id/ac_owner_company"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp20"
                            android:text="Airtel"
                            android:textColor="@color/black" />

                    </LinearLayout>

                    <LinearLayout
                        style="@style/ColumnItemInfo"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content">

                        <TextView
                            style="@style/RowTextItemInfo"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="User Company" />

                        <TextView
                            android:id="@+id/ac_user_company"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp20"
                            android:text="Airtel"
                            android:textColor="@color/black" />

                    </LinearLayout>


                </GridLayout>

            </LinearLayout>
        </LinearLayout>

        <View
            android:id="@+id/itemLine"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:alpha="0.37"
            android:background="#263238" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>