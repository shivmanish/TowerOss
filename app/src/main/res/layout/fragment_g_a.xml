<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/site_detail_background"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragments.qat.GAFragment">
    <!-- TODO: Update blank fragment layout -->
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:id="@+id/rlParent1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageView
                android:id="@+id/img_back"
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:paddingTop="6dp"
                android:paddingEnd="8dp"

                android:paddingBottom="6dp"


                app:srcCompat="@drawable/ic_back"
                app:tint="@color/iconBlack" />
            <TextView
                android:id="@+id/qa"
                android:fontFamily="@font/montserrat_bold"

                android:textColor="#000000"
                android:layout_margin="@dimen/activity_horizontal_margin"
                android:layout_toRightOf="@+id/img_back"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_alignParentTop="true"
                android:layout_marginTop="@dimen/activity_horizontal_margin"
                android:gravity="top"
                android:lineSpacingExtra="-7sp"
                android:text="@string/qa"
                android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

        </RelativeLayout>
        <RelativeLayout

            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/electr"
                android:fontFamily="@font/montserrat_bold"
                android:textColor="#000000"
                android:layout_margin="@dimen/activity_horizontal_margin"

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_alignParentTop="true"
                android:layout_marginTop="@dimen/activity_horizontal_margin"
                android:gravity="top"
                android:lineSpacingExtra="-7sp"
                android:text="@string/electircal_civil_material"
                android:textAppearance="@style/TextAppearance.AppCompat.Medium" />
            <TextView
                android:id="@+id/textelecSubtitle"
                android:layout_below="@+id/electr"
                android:fontFamily="@font/montserrat_bold"
                android:layout_marginLeft="@dimen/activity_horizontal_margin"
                android:textColor="@color/grey"

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginTop="@dimen/activity_horizontal_margin"
                android:text="* Pole &amp; Mount Material > Pole -->"
                android:textAppearance="@style/TextAppearance.AppCompat.Medium" />
        </RelativeLayout>
        <androidx.cardview.widget.CardView
            android:layout_margin="@dimen/activity_horizontal_margin"
            android:layout_width="match_parent"
            android:background="@drawable/bgwhiterec"
            android:layout_height="wrap_content">
           <LinearLayout
               android:orientation="vertical"
               android:layout_width="match_parent"
               android:layout_height="wrap_content">
               <RelativeLayout
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content">

                   <TextView
                       android:id="@+id/txtCardHeading"
                       android:layout_width="wrap_content"

                       android:layout_height="wrap_content"
                       android:layout_alignParentTop="true"
                       android:layout_centerVertical="true"
                       android:layout_marginStart="@dimen/activity_horizontal_margin"
                       android:layout_marginTop="@dimen/activity_horizontal_margin"
                       android:layout_marginEnd="@dimen/activity_horizontal_margin"
                       android:layout_marginBottom="@dimen/activity_horizontal_margin"
                       android:fontFamily="@font/montserrat_bold"
                       android:gravity="top"
                       android:lineSpacingExtra="-7sp"
                       android:text="1."
                       android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                       android:textColor="#000000"
                       android:textSize="16sp" />

                   <TextView
                       android:id="@+id/txtCardHeading2"
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       android:layout_alignParentTop="true"
                       android:layout_centerVertical="true"
                       android:layout_marginStart="@dimen/activity_horizontal_margin"
                       android:layout_marginTop="@dimen/activity_horizontal_margin"
                       android:layout_marginEnd="@dimen/activity_horizontal_margin"
                       android:layout_marginBottom="@dimen/activity_horizontal_margin"
                       android:layout_toRightOf="@+id/txtCardHeading"
                       android:fontFamily="@font/montserrat_bold"
                       android:gravity="top"
                       android:lineSpacingExtra="-7sp"
                       android:text="Pole Height"
                       android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                       android:textColor="#000000"
                       android:textSize="16sp" />
                   <TextView
                       android:layout_centerVertical="true"
                       android:layout_toRightOf="@+id/txtCardHeading2"
                       android:layout_width="@dimen/dp30"
                       android:layout_height="@dimen/dp30"
                       android:text="i"
                       android:textSize="12sp"
                       android:background="@drawable/rounded_textview"
                       android:textColor="@android:color/white"
                       android:gravity="center"
                       android:id="@+id/mark" />

               </RelativeLayout>
               <com.google.android.material.card.MaterialCardView
                   android:layout_below="@+id/txtCardHeading"
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content"
                   app:strokeWidth="1dp"
                   android:layout_margin="@dimen/activity_horizontal_margin"
                   app:strokeColor="#D3D3D3"
                   app:cardCornerRadius="4dp">

                   <androidx.appcompat.widget.AppCompatSpinner
                       android:id="@+id/spinner"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:padding="20dp"/>

               </com.google.android.material.card.MaterialCardView>
               <com.google.android.material.card.MaterialCardView
                   android:layout_below="@+id/txtCardHeading"
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content"
                   app:strokeWidth="1dp"
                   android:layout_margin="@dimen/activity_horizontal_margin"
                   app:strokeColor="#D3D3D3"
                   app:cardCornerRadius="4dp">

                   <androidx.appcompat.widget.AppCompatSpinner
                       android:id="@+id/spinner2"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:padding="20dp"/>

               </com.google.android.material.card.MaterialCardView>
                 <FrameLayout
                      android:background="@drawable/bggraycard"
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content">
                    <LinearLayout
                        android:weightSum="1"
                        android:layout_marginLeft="@dimen/activity_horizontal_margin"
                        android:layout_marginRight="@dimen/activity_horizontal_margin"

                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TextView
                            android:id="@+id/mark_taylor"
                            android:layout_weight=".60"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/activity_horizontal_margin"
                            android:layout_marginTop="@dimen/activity_horizontal_margin"
                            android:fontFamily="@font/montserrat_bold"
                            android:textColor="#000000"
                            android:gravity="center_vertical"
                            android:lineSpacingExtra="8sp"
                            android:text="@string/mark_taylor"
                            android:textAppearance="@style/TextAppearance.AppCompat.Medium" />
                        <TextView

                            android:id="@+id/txtDate"
                            android:layout_weight=".30"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/activity_horizontal_margin"
                            android:layout_marginTop="@dimen/activity_horizontal_margin"
                             android:fontFamily="@font/montserrat_bold"
                            android:textColor="#000000"
                            android:gravity="end"
                            android:lineSpacingExtra="8sp"
                            android:text="03-Oct-22 ; 12:56 PM"
                            android:textAppearance="@style/TextAppearance.AppCompat.Medium" />

                    </LinearLayout>
   <!--                  <Button
                         xmlns:android="http://schemas.android.com/apk/res/android"
                         android:id="@+id/get_button"

                         android:layout_width="match_parent"
                         android:layout_height="@dimen/dp60"
                         android:layout_alignParentLeft="true"
                         android:layout_alignParentRight="true"
                         android:layout_alignParentTop="true"
                         android:layout_alignParentBottom="true"
                         android:background="@drawable/get_button"

                         />-->


                 </FrameLayout>
           </LinearLayout>





        </androidx.cardview.widget.CardView>

    </LinearLayout>





</FrameLayout>