<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/site_detail_background"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.gat.GAFragment">
    <!-- TODO: Update blank fragment layout -->
 <LinearLayout
     android:orientation="vertical"
     android:layout_width="match_parent"
     android:layout_height="wrap_content">
     <RelativeLayout
         android:id="@+id/rlParent1"
         android:layout_width="match_parent"
         android:layout_height="wrap_content">
         <ImageView
             android:id="@+id/img_back"
             android:layout_centerVertical="true"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_marginStart="16dp"
             android:paddingTop="6dp"
             android:paddingEnd="8dp"

             android:paddingBottom="6dp"


             app:srcCompat="@drawable/ic_back"
             app:tint="@color/iconBlack" />
         <TextView
             android:id="@+id/qa"
             android:fontFamily="@font/montserrat_bold"

             android:textColor="#000000"
             android:layout_margin="@dimen/activity_horizontal_margin"
             android:layout_toRightOf="@+id/img_back"
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:layout_centerVertical="true"
             android:layout_alignParentTop="true"
             android:layout_marginTop="@dimen/activity_horizontal_margin"
             android:gravity="top"
             android:lineSpacingExtra="-7sp"
             android:text="@string/qa"
             android:textAppearance="@style/TextAppearance.AppCompat.Display1" />

     </RelativeLayout>
     <RelativeLayout
         android:layout_width="match_parent"
         android:layout_height="wrap_content">
         <TextView
             android:id="@+id/electr"
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:layout_alignParentTop="true"
             android:layout_centerVertical="true"
             android:layout_marginStart="@dimen/activity_horizontal_margin"
             android:layout_marginTop="@dimen/activity_horizontal_margin"
             android:layout_marginEnd="@dimen/activity_horizontal_margin"
             android:layout_marginBottom="@dimen/activity_horizontal_margin"
             android:fontFamily="@font/montserrat_bold"
             android:gravity="top"
             android:lineSpacingExtra="-7sp"
             android:text="@string/electircal_civil_material"
             android:textAppearance="@style/TextAppearance.AppCompat.Medium"
             android:textColor="#000000" />
         <TextView
             android:id="@+id/textelecSubtitle"
             android:layout_below="@+id/electr"
             android:fontFamily="@font/montserrat_bold"
             android:layout_marginLeft="@dimen/activity_horizontal_margin"
             android:textColor="@color/grey"
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:layout_gravity="center_vertical"
             android:layout_marginTop="@dimen/activity_horizontal_margin"
             android:text="* Pole &amp; Mount Material > Pole -->"
             android:textAppearance="@style/TextAppearance.AppCompat.Medium" />
     </RelativeLayout>
     <androidx.recyclerview.widget.RecyclerView
         android:orientation="vertical"
         android:id="@+id/rvDynamicList"
         android:layout_width="match_parent"
         android:layout_height="wrap_content"
         app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager">
     </androidx.recyclerview.widget.RecyclerView>
 </LinearLayout>
</FrameLayout>